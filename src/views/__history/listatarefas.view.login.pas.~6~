{$R *.dfm}

procedure TfrmLogin.Button1Click(Sender: TObject);
var
  lCadUsuario: TfrmCadUsuario;
begin
  lCadUsuario:= TfrmCadUsuario.Create(nil);
  try

  finally
    FreeAndNil(lCadUsuario);
  end;
end;

